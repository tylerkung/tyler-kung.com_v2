<footer>
    <div class="footer">
        <div class="row">
            <div class="col-sm-6">
                <ul class="footer-nav">
					{{#each footer-navigation}}
						{{#if socials}}
							<li class="socials">
								{{#each socials}}
									<span><a href="{{link}}"><i class="fab {{icon}}"></i></a></span>
								{{/each}}
							</li>
                        {{else}}
                        <li><a href="{{ link }}">{{ title }}</a></li>
						{{/if}}

					{{/each}}
                </ul>
            </div>
        </div>
    </div>
</footer>
<script src="/scripts/all.js"></script>
